{
  "name": "multitude",
  "productName": "Multitude",
  "version": "0.0.1",
  "prerelease": true,
  "description": "The missing desktop client for streaming music sites",
  "scripts": {
    "prestart": "npm run lint && webpack",
    "start": "electron bin/app/index.js",
    "prepackage:darwin": "webpack",
    "package:darwin": "electron-packager . --icon=assets/icons/mac/icon.ico --overwrite --ignore=__.+ --ignore=tmp --ignore=assets  --ignore=src",
    "test": "standard",
    "lint": "eslint src --ignore-path .gitignore -c .eslint",
    "install-all": "echo ':multitude' && npm install && cd src/app && echo ':multitude-app' && npm install && cd ../../src/scenes/musicboxes && echo ':multitude-scenes-musicboxes' && npm install",
    "outdated-all": "echo ':multitude' && npm outdated || true && cd src/app && echo ':multitude-app' && npm outdated || true && cd ../../src/scenes/musicboxes && echo ':multitude-scenes-musicboxes' && npm outdated || true",
    "dev:platform": "webpack --task=platform && electron bin/app/index.js",
    "dev:app": "webpack --task=app && electron bin/app/index.js",
    "dev:musicboxes": "webpack --task=musicboxes && electron bin/app/index.js",
    "dev:assets": "webpack --task=assets && electron bin/app/index.js",
    "dev:run": "electron bin/app/index.js",
    "dev:icons": "electron-icon-maker --input=./assets/appIcon.png --output=./assets"
  },
  "keywords": [],
  "author": {
    "name": "George McGinley Smith",
    "email": "george@gsgd.co.uk",
    "url": "http://gsgd.co.uk"
  },
  "contributors": [
    "Thomas Beverley"
  ],
  "homepage": "https://gsgd.github.io/multitude/",
  "license": "MPL-2.0",
  "repository": "https://github.com/gsgd/multitude",
  "main": "bin/app/index.js",
  "dependencies": {
    "babel": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "6.3.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "clean-webpack-plugin": "0.1.15",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "electron": "^1.7.9",
    "electron-icon-maker-fix": "0.0.5",
    "electron-packager": "^9.1.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "json-loader": "0.5.4",
    "jsx-loader": "0.13.2",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "lodash.throttle": "^4.1.1",
    "shared": "file:bin/app/shared",
    "style-loader": "0.13.1",
    "uglify-js": "^3.1.3",
    "url-loader": "0.5.7",
    "uuid": "3.0.1",
    "webpack": "1.14.0",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.4.0",
    "standard": "8.6.0"
  },
  "standard": {
    "ignore": [
      "src/app/lib/"
    ]
  }
}
