{
  "name": "musicbox",
  "productName": "MusicBox",
  "version": "2.3.1",
  "prerelease": true,
  "description": "The missing desktop client for streaming music sites",
  "scripts": {
    "prestart": "webpack",
    "start": "electron bin/app/index.js",
    "test": "standard",
    "lint": "eslint src --ignore-path .gitignore -c .eslint",
    "install-all": "echo ':wmail' && npm install && cd src/app && echo ':wmail-app' && npm install && cd ../../src/scenes/musicboxes && echo ':wmail-scenes-musicboxes' && npm install",
    "outdated-all": "echo ':wmail' && npm outdated && cd src/app && echo ':wmail-app' && npm outdated && cd ../../src/scenes/musicboxes && echo ':wmail-scenes-musicboxes' && npm outdated",
    "dev:platform": "webpack --task=platform && electron bin/app/index.js",
    "dev:app": "webpack --task=app && electron bin/app/index.js",
    "dev:musicboxes": "webpack --task=musicboxes && electron bin/app/index.js",
    "dev:assets": "webpack --task=assets && electron bin/app/index.js",
    "dev:run": "electron bin/app/index.js"
  },
  "keywords": [],
  "author": {
    "name": "George McGinley Smith",
    "email": "george@gsdg.co.uk",
    "url": "http://gsgd.co.uk"
  },
  "homepage": "https://thomas101.github.io/wmail/",
  "license": "MPL-2.0",
  "repository": "https://github.com/Thomas101/wmail",
  "main": "bin/app/index.js",
  "dependencies": {
    "babel": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "^8.0.1",
    "babel-loader": "6.3.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "clean-webpack-plugin": "0.1.15",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "electron": "1.6.1",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.10.0",
    "json-loader": "0.5.4",
    "jsx-loader": "0.13.2",
    "less": "2.7.2",
    "less-loader": "2.2.3",
    "style-loader": "0.13.1",
    "uglify-js": "mishoo/UglifyJS2#3ee46e91e802fb8bf20656bce115375c5f624052",
    "url-loader": "0.5.7",
    "uuid": "3.0.1",
    "webpack": "1.14.0",
    "webpack-target-electron-renderer": "0.4.0"
  },
  "devDependencies": {
    "eslint": "^4.8.0",
    "eslint-plugin-react": "^7.4.0",
    "standard": "8.6.0"
  },
  "standard": {
    "ignore": [
      "src/app/lib/"
    ]
  }
}
